{
  "name": "apollo-passport-react",
  "version": "0.0.3",
  "description": "React UI for apollo-passport, like Meteor Accounts loginButtons",
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel -d lib/ src/",
    "prepublish": "npm run compile",
    "test": "mocha --compilers js:babel-core/register \"src/**/*.spec.js\"",
    "coverage": "istanbul cover _mocha -- --compilers js:babel-core/register \"src/**/*.spec.js\" -R spec",
    "coveralls": "istanbul cover _mocha --report lcovonly -- --compilers js:babel-core/register \"src/**/*.spec.js\" -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gadicc/apollo-passport-react"
  },
  "bugs": {
    "url": "https://github.com/gadicc/apollo-passport-react/issues"
  },
  "keywords": [
    "apollo",
    "passport",
    "auth",
    "authentication",
    "react",
    "apollo-passport-ui-framework"
  ],
  "author": "Gadi Cohen",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.0.4",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-regenerator": "^6.9.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.10.2",
    "eslint-plugin-jsx-control-statements": "^2.1.1",
    "eslint-plugin-react": "^5.1.1",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^2.5.3",
    "mocha-lcov-reporter": "^1.2.0",
    "proxyquire": "^1.7.10",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "regenerator-runtime": "^0.9.5",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0"
  },
  "peerDependencies": {
    "graphql-tag": "^0.1.7",
    "regenerator-runtime": "^0.9.5",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2"
  },
  "dependencies": {
    "jsx-control-statements": "^3.1.2",
    "invariant": "^2.0.0"
  }
}
